{
  "name": "url",
  "version": "1.0.0",
  "description": "Parse urls to objects and objects to urls",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "url parser",
    "url",
    "parser"
  ],
  "scripts": {
    "build:dev": "parcel src/index.ts --global url --out-dir dist_dev",
    "build:prod": "parcel build src/index.ts --global url --out-dir dist",
    "dev": "concurrently \"npm run build:dev\" \"npm run test\" --kill-others --restartTries 2 --restartDelay 200",
    "test": "jest --watchAll",
    "tt": "tsc src/index.ts --emitDeclarationOnly --declaration true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabien-h/url.git"
  },
  "author": "https://twitter.com/fabien_huet",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "concurrently": "^4.1.0",
    "jest": "^23.6.0",
    "parcel": "^1.10.3",
    "parcel-bundler": "^1.10.3",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2"
  }
}
